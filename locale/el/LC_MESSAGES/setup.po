msgid "yes"
msgstr "ναι"

#: module/setup.py:489
msgid "true"
msgstr "σωστό"

#: module/setup.py:489
msgid "t"
msgstr "t"

#: module/setup.py:492
msgid "no"
msgstr "όχι"

#: module/setup.py:492
msgid "false"
msgstr "λάθος"

